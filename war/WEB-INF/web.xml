<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
      version="3.0"> 
	<servlet>
		<servlet-name>UploadImage</servlet-name>
		<servlet-class>de.TUBlr.manager.web.Upload</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadImage</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ServeImage</servlet-name>
		<servlet-class>de.TUBlr.manager.web.Serve</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServeImage</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>
		
	<servlet>
		<servlet-name>TUBlr</servlet-name>
		<servlet-class>de.TUBlr.manager.web.TUBlrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TUBlr</servlet-name>
		<url-pattern>/TUBlr</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TUBlr</servlet-name>
		<url-pattern>/tublr</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>tublr.jsp</welcome-file>
	</welcome-file-list>
</web-app>
